{
  "name": "@investment-platform/cdn-management",
  "version": "1.0.0",
  "description": "CDN management service for static assets optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "docker:build": "docker build -t investment-platform/cdn-management .",
    "docker:run": "docker run -p 3009:3009 investment-platform/cdn-management"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@azure/storage-blob": "^12.17.0",
    "@google-cloud/storage": "^7.7.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.2",
    "mime-types": "^2.1.35",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/mime-types": "^2.1.4",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "investment-platform",
    "cdn",
    "static-assets",
    "performance",
    "optimization",
    "caching"
  ],
  "author": "Investment Platform Team",
  "license": "MIT"
}